        if ($rails_env == 'development' || $rails_env == 'test') {
            var query_string     = null,
                subdomain_number = '0';
        } else {
            var query_string     = $app_rev,
                subdomain_number = parseInt($app_rev.substring(0, 10), 16) % 6;
        }

        var convert_to_asset_url = function (asset_name) {
            if (asset_name.match(/^(https?:)?\/\//)) {
                return asset_name;
            }

            var name = "";
            if (asset_name[0] == "/") {
                name = asset_name;
            } else {
                name = "/javascripts/" + asset_name + ".js";
            }

            if ($rails_env != 'development' && $rails_env != 'test' && digest_map[name]) {
                return digest_map[name];
            } else {
                return ('//' + subdomain_number + '.' +
                    $asset_host + name + '?' + query_string);
            }
        }

        window.req = function (deps, cb) {
            if (!_.isArray(deps)) { deps = [deps] };

            deps = _.reject(deps, function (str) {
                return _.include(req.ignored, str);
            });

            require(_.map(deps, convert_to_asset_url), cb);
        };

        window.req.ignored = ["mini_registration"];

        window.req_and_ready = function (deps, cb) {
            req(deps, function () { $(cb); });
        };
    })();
  </script>

