<script type="text/javascript">
var scopes = 'openid email https://www.googleapis.com/auth/userinfo.profile https://www.google.com/m8/feeds/ https://www.googleapis.com/auth/plus.me';
var clientId = '331998490334-rsn3chp12mbkiqhl6e7lu2q0mlbu0f1b';
function loadGoogle() {
  Google.client_id = clientId;
  Google.scopes = scopes;
  gapi.client.setApiKey('AIzaSyB924RA9X5fW1hDUt8BUfLNqzPKdno5W5c');
  gapi.auth.init();
};

window.googleAsyncLoad = function() {
    if (window.gapi) {
        return;
    }
    (function(d) {
        var js;
        var id = 'google-jssdk';
        var ref = d.getElementsByTagName('script')[0];
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement('script');
        js.id = id;
        js.async = true;
        js.src = "https://apis.google.com/js/client.js?onload=loadGoogle"
        ref.parentNode.insertBefore(js, ref);
    }(document));
}

if (!window.defer_google) {
    window.googleAsyncLoad();
} else {
    setTimeout(function(){
        window.googleAsyncLoad();
    }, 5000);
}
</script>



  



